#regle include permet d'inclure des bouts de makefile(/code?) a l'interieur du makefile en prenant l'adresse de celui ci

.PHONY : clean, fclean, re, all $(NAME)
.SUFFIXES :

NAME = test_libasm
#name of the project

DEBUG = no

OPTI_FLAGS = -O2
DEBUG_FLAG = -fsanitize=address -pedantic -Weverything -std=c89

CC = gcc
FLAG = -Wall -Wextra -Werror

ifeq ($(DEBUG), yes)
        SPE_FLAGS = $(DEBUG_FLAG) $(OPTI_FLAGS)
else
        SPE_FLAGS =
endif

SRC_DIR = ./
SRC = main_test.c \
	test_bzero.c \
	test_cat.c \
	test_isalnum.c \
	test_isalpha.c \
	test_isascii.c \
	test_isdigit.c \
	test_isprint.c \
	test_memcpy.c \
	test_memcmp.c \
	test_strcmp.c \
	test_memchr.c \
	test_memset.c \
	test_puts.c \
	test_strcat.c \
	test_strdup.c \
	test_strlen.c \
	test_memalloc.c \
	test_strncat.c \
	test_swap.c \
	test_tolower.c \
	test_toupper.c

LIB_DIR = ../
LIB = $(LIB_DIR)libfts.a

OBJ_DIR = ./
OBJ = $(SRC:.c=.o)

INC = ./includes/
INC_FILES = # ls -1 > src.txt &&

AUTOR = auteur

all : $(NAME)

$(NAME) : $(LIB) $(OBJ_DIR) $(addprefix $(OBJ_DIR), $(OBJ)) $(addprefix $(INC), $(INC_FILES)) $(AUTOR)
	@($(CC) $(FLAGS) $(SPE_FLAGS) $(addprefix $(OBJ_DIR), $(OBJ)) $(LIB) -I$(INC) -o $(NAME))
#	@(ar rc $(NAME) $(addprefix $(OBJ_DIR), $(OBJ)))
#	@(ranlib $(NAME))
	$(echo creation de $(NAME))

$(LIB) :
	@(cd $(LIB_DIR) && MAKE)

$(OBJ_DIR)%.o : $(addprefix $(SRC_DIR), %.c) $(addprefix $(INC), $(INC_FILES))
	@($(CC) $(FLAG) -I $(INC) -c $< -o $@)
	$(echo $<)

$(OBJ_DIR) :
	@(mkdir $(OBJ_DIR))

$(AUTOR) :
	@(echo "gnebie" > ./auteur;)

clean :
	#@(cd $(NM_DIR) && $(MAKE) $@)
	@(rm -f $(addprefix $(OBJ_DIR), $(OBJ)))
	@(echo suppression des .o)

fclean : clean
	@(rm -f $(NAME))
	@(echo suppression de $(NAME))

re : fclean all

test : re
	./test_libasm

